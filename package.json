{
  "name": "online-judge-platform",
  "version": "1.0.0",
  "description": "Online Judge Platform with Node.js backend and React frontend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "start": "concurrently \"npm run backend:start\" \"npm run frontend:start\"",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm run start",
    "backend:install": "cd backend && npm install",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:start": "cd frontend && npm run start",
    "frontend:install": "cd frontend && npm install",
    "install:all": "npm run backend:install && npm run frontend:install",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "online-judge",
    "coding-platform",
    "nodejs",
    "react",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}